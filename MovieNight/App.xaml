<Application
    x:Class="MovieNight.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MovieNight"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/Skins/DarkMode.xaml" />
                <ResourceDictionary Source="/Styles/MenuStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="primaryFont">pack://application:,,,/Fonts/#Bebas Neue</FontFamily>

        </ResourceDictionary>
    </Application.Resources>
</Application>
