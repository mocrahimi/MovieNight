<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Background Colors  -->
    <Color x:Key="primaryColor">#221f1f</Color>
    <Color x:Key="ascendColor1">#b4d429</Color>
    <Color x:Key="ascendColor2">#e50914</Color>
    <Color x:Key="ascendColor3">#f5f5f1</Color>
    <Image x:Key="BackgroundImg" Source="/Resources\Images\Background.png" />

    <!--  Window Border Colors  -->
    <Color x:Key="winBorderColor1">#6D2FFF</Color>
    <Color x:Key="winBorderColor2">#FB539B</Color>
    <Color x:Key="winBorderColor3">#836EFB</Color>
    <!--  Color Palette  -->
    <SolidColorBrush x:Key="color0" Color="#181818" />
    <SolidColorBrush x:Key="color1" Color="#b4d429" />
    <SolidColorBrush x:Key="color2" Color="#b9d839" />


    <!--  Font Colors  -->
    <SolidColorBrush x:Key="TitleColor" Color="WhiteSmoke" />
    <SolidColorBrush x:Key="CaptionColor" Color="#E9E9E9" />
    <SolidColorBrush x:Key="HoverColor" Color="#252525" />
    <SolidColorBrush x:Key="ActiveColor" Color="#181818" />

    <!--  Panel Colors  -->
    <SolidColorBrush x:Key="panelColor" Color="#200F53" />
    <SolidColorBrush x:Key="panelOverColor" Color="#281269" />
    <SolidColorBrush x:Key="panelActiveColor" Color="#2B1372" />
    <!--  Button Colors  -->
    <!--  TextBox Colors  -->
    <!--  Etc  -->
</ResourceDictionary>